{
  "name": "keyworks-sample",
  "description": "sample",
  "version": "0.0.1",
  "author": "yinm",
  "background": {
    "scripts": [
      "assets/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png"
    },
    "default_title": "keyworks-sample"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "images/16.png",
    "19": "images/19.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "clipboardRead",
    "storage"
  ]
}
